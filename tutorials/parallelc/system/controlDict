FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         40e-3;

deltaT          5e-3;

writeControl    timeStep;

writeInterval   2;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


functions
{

  exportH5
    {
      type     exportToHDF5;
      functionObjectLibs ( "libhdf5.so" "libexportToHDF5.so");
      outputControl outputTime;//timeStep;
      //      outputInterval 1;
      filename "results.h5foam";
      fields (p U);
    }

}
